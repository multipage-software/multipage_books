      <segment id="2-0">
        <source xml:space="preserve"> Tato značka slouží ke vkládání poznámek k programu.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Na místo této značky se do internetové stránky vloží prázdné místo.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> [@REM] Moje poznámka.</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [/@REM] , kde předchozí značka je v textu nahrazena prázdným místem a nemá žádný vliv na generovanou stránku.</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> Příkazu [@REM] lze také využít pro dočasné vypnutí částí HTML, CSS, JavaScript a PHP kódu.</source>
      </segment>
      <segment id="2-6">
        <source xml:space="preserve"> Například následující PHP kód:</source>
      </segment>
      <segment id="2-7">
        <source xml:space="preserve"> &lt;?php $value = 3.14; [@REM]$value = $value / 4;[/@REM] echo $value; ?&gt; - vypíše hodnotu:</source>
      </segment>
      <segment id="2-8">
        <source xml:space="preserve"> .</source>
      </segment>
      <segment id="2-9">
        <source xml:space="preserve"> <ph id="ph0"/><ph id="ph1"/> Řádek s dělením číslem čtyři je díky značce [@REM] vynechán.</source>
