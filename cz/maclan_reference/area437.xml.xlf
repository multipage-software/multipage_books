    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Tato složená značka odstraňuje prázdné znaky z textu uvnitř značky a výsledek vkládá do stránky.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Není-li uveden žádný parametr, jsou souvislé oblasti s prázdnými znaky nahrazeny vždy jedinou mezerou.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> V případě, že je uveden parametr strong, jsou z textu odstraněny všechny prázdné znaky.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Následující příklad odstraní souvislé oblasti s prázdnými znaky z textu:</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [@PACK] 1 2 3 4 5 [/@PACK] - výsledkem je text:</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> 1 2 3 4 5 </source>
      </segment>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@PACK]...[/@PACK] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> strong:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Je-li ve značce použitý parametr strong, odstraní se všechny prázdné znaky z textu uvnitř značky.</source>
      </segment>
      <segment id="5-1">
        <source xml:space="preserve"> Například následující program:</source>
      </segment>
      <ignorable id="5-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> [@PACK strong] A B C D E [/@PACK] </source>
      </segment>
    </unit>
