    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Není definováno v JavaScriptu.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Tato funkce vytváří nový seznam typu LinkedList.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Hodnoty parametrů funkce (jsou nepovinné) jsou vloženy ve stejném pořadí do seznamu.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Následující příkaz vytvoří nový prázdný seznam s názvem myList:</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [@VAR myList=newList()] Další příkaz vytvoří nový seznam s názvem myList a vloží do něj hodnoty 1, 'abcd' a true:</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> [@VAR myList="newList(1, 'abcd', true)"] </source>
      </segment>
