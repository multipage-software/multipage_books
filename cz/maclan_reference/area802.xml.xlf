    <unit id="1">
      <segment id="1-0">
        <source xml:space="preserve"> [@BOOKMARK] </source>
      </segment>
    </unit>
    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Značka definuje místo, kde bude vložen text vytvořený v jiné části programu pomocí [@REPLACE_BOOKMARK].</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Parametr name nebo název prvního parametru učuje název této záložky.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Následující příklad ilustruje použití značek [@BOOKMARK] a [@REPLACE_BOOKMARK]:</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Toto je [@BOOKMARK muj_text].</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> --- [@REPLACE_BOOKMARK muj_text] oblast "[@AREA_NAME homeArea]" [/@REPLACE_BOOKMARK] - vypíše na stránku text:</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> Toto je oblast "Dokumentace k jazyku Maclan" .</source>
      </segment>
      <segment id="2-6">
        <source xml:space="preserve"> --- Jak je vidět v předchozím příkladu, lze text vkládat do již provedené části stránky.</source>
      </segment>
      <segment id="2-7">
        <source xml:space="preserve"> Použije-li se parametr name, pak bude předchozí příklad vypadat takto:</source>
      </segment>
      <segment id="2-8">
        <source xml:space="preserve"> Toto je [@BOOKMARK name=#muj_text].</source>
      </segment>
      <segment id="2-9">
        <source xml:space="preserve"> --- [@REPLACE_BOOKMARK name=#muj_text] oblast "[@AREA_NAME homeArea]" [/@REPLACE_BOOKMARK] - vypíše na stránku opět text:</source>
      </segment>
      <segment id="2-10">
        <source xml:space="preserve"> Toto je oblast "Dokumentace k jazyku Maclan" .</source>
      </segment>
      <segment id="2-11">
        <source xml:space="preserve"> --- </source>
