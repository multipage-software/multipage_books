    <unit id="1">
      <segment id="1-0">
        <source xml:space="preserve"> [@PRAGMA] </source>
      </segment>
    </unit>
    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Pomocná značka s informacemi pro generátor stránek.</source>
      </segment>
      <ignorable id="2-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@PRAGMA] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> php:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Parametr php uvádí zda májí být zpracovány PHP příkazy uvedené v programu stránky.</source>
      </segment>
      <segment id="5-1">
        <source xml:space="preserve"> Je-li hodnota tohoto parametru true, PHP příkazy se zpracují pomocí interpreteru a v případě, že je hodnota tohoto parametru false, příkazy se neprovedou a zůstávají na stránce.</source>
      </segment>
      <segment id="5-2">
        <source xml:space="preserve"> Například následující program:</source>
      </segment>
      <ignorable id="5-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> [@PRAGMA php=false] </source>
      </segment>
    </unit>
    <unit id="7">
      <segment id="7-0">
        <source xml:space="preserve"> - vypíše na stránku text:</source>
      </segment>
      <segment id="7-1">
        <source xml:space="preserve"> &lt;?php echo "Good morming."; ?&gt;.</source>
      </segment>
      <segment id="7-2">
        <source xml:space="preserve"> Příkaz jazyka PHP se v tomto případě neprovede.</source>
      </segment>
      <ignorable id="7-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="8">
      <segment id="8-0">
        <source xml:space="preserve"> tabulator:</source>
      </segment>
      <ignorable id="8-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="9">
      <segment id="9-0">
        <source xml:space="preserve"> Parametr tabulator nastavuje implicitní hodnotu tabulátoru.</source>
      </segment>
      <segment id="9-1">
        <source xml:space="preserve"> Tato hodnota je použita ve značce [@INDENT].</source>
      </segment>
      <segment id="9-2">
        <source xml:space="preserve"> Například následující program:</source>
      </segment>
      <ignorable id="9-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="10">
      <segment id="10-0">
        <source xml:space="preserve"> [@PRAGMA tabulator=#&gt;&gt;&gt;] [@INDENT] muj text [/@INDENT] </source>
      </segment>
    </unit>
    <unit id="11">
      <segment id="11-0">
        <source xml:space="preserve"> - vypíše na stránku text:</source>
      </segment>
      <segment id="11-1">
        <source xml:space="preserve"> &gt;&gt;&gt;muj text </source>
      </segment>
    </unit>
    <unit id="12">
      <segment id="12-0">
        <source xml:space="preserve"> webInterface:</source>
      </segment>
      <ignorable id="12-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="13">
      <segment id="13-0">
        <source xml:space="preserve"> Parametr webInterface nastavuje novou hodnotu pro adresář webového rozhraní.</source>
      </segment>
      <segment id="13-1">
        <source xml:space="preserve"> Tato hodnota je použita po ukončení programu metajazyka pro přesměrování na statickou stránku.</source>
      </segment>
      <ignorable id="13-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="14">
      <segment id="14-0">
        <source xml:space="preserve"> [@PRAGMA webInterface=#prestashop] </source>
      </segment>
    </unit>
    <unit id="15">
      <segment id="15-0">
        <source xml:space="preserve"> - po ukončení programu metajazyka se nastaví nový adresář webového rozhraní na hodnotu prestashop.</source>
      </segment>
      <segment id="15-1">
        <source xml:space="preserve"> Přesměrování na stránku v tomto novém adresáři lze provést pomocí příkazu [@REDIRECT].</source>
      </segment>
      <ignorable id="15-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="16">
      <segment id="16-0">
        <source xml:space="preserve"> metaCharset:</source>
      </segment>
      <ignorable id="16-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="17">
      <segment id="17-0">
        <source xml:space="preserve"> Parametr metaCharset může nabývat hodnot true a false.</source>
      </segment>
      <segment id="17-1">
        <source xml:space="preserve"> Určuje zda bude do sekce stránky umístěna značka určující její kódování (&lt;meta http-equiv="Content-Type" content="text/html; charset=.."&gt;).</source>
      </segment>
      <ignorable id="17-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="18">
      <segment id="18-0">
        <source xml:space="preserve"> [@PRAGMA metaCharset=true] </source>
      </segment>
    </unit>
  </file>
