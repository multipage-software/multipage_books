    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Jedná se o složenou značku.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Značka [@LANGUAGES] provádí výčet přes všechny dostupné jazyky internetových stránek.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Na stránku bude vložen text, který se nachází uvnitř značky přesně tolikrát, kolik existuje jazyků.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Ve vnitřním textu této značky se mohou nacházet značky [@LANG_FLAG], [@LANG_DESCRIPTION], [@LANG_ALIAS] a [@LANG_ID], které vkládají údaje o aktuálním jazyku zvoleném značkou [@LANGUAGES].</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> Například následující program vloží na stránku seznam vlajek jazyků ve složených závorkách:</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> [@LANGUAGES] { [@LANG_FLAG] } [/@LANGUAGES] Výsledek předchozích příkazů je následující:</source>
      </segment>
      <segment id="2-6">
        <source xml:space="preserve"> { } </source>
      </segment>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@LANGUAGES] ...</source>
      </segment>
      <segment id="3-1">
        <source xml:space="preserve"> [/@LANGUAGES] Má následující parametry:</source>
      </segment>
      <ignorable id="3-2">
        <source xml:space="preserve"> </source>
      </ignorable>
