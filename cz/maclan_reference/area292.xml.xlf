    <unit id="1">
      <segment id="1-0">
        <source xml:space="preserve"> [@SUPERAREAS] </source>
      </segment>
    </unit>
    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Jednoduchá značka [@SUPERAREAS] je nahrazena prázdným řetězcem (nevypisuje se nic).</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Pomocí této značky ale máte možnost definovat lokální proměnnou, jejíž hodnotou je seznam nadoblastí aktuální nebo zvolené oblasti (viz. následující seznam parametrů).</source>
      </segment>
      <ignorable id="2-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@SUPERAREAS] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> list:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Parametr list pojmenovává novou proměnnou typu LinkedList, které bude přiřazen odpovídající seznam oblastí.</source>
      </segment>
      <segment id="5-1">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="5-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> [@SUPERAREAS list = my_list] </source>
      </segment>
    </unit>
    <unit id="7">
      <segment id="7-0">
        <source xml:space="preserve"> vytvoří lokální proměnnou s názvem my_list, jejíž hodnotou je seznam typu LinkedList obsahující odpovídající seznam oblastí.</source>
      </segment>
      <segment id="7-1">
        <source xml:space="preserve"> Tuto proměnnou lze pak používat v dalších příkazech.</source>
      </segment>
      <ignorable id="7-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="8">
      <segment id="8-0">
        <source xml:space="preserve"> first:</source>
      </segment>
      <ignorable id="8-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="9">
      <segment id="9-0">
        <source xml:space="preserve"> Parametr vytvoří proměnnou typu Area, která odkazuje na první prvek daného seznamu oblastí.</source>
      </segment>
      <segment id="9-1">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="9-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="10">
      <segment id="10-0">
        <source xml:space="preserve"> [@SUPERAREAS first=my_area] </source>
      </segment>
    </unit>
    <unit id="11">
      <segment id="11-0">
        <source xml:space="preserve"> vytvoří proměnnou my_area jejíž hodnotou je první oblast z daného seznamu oblastí.</source>
      </segment>
      <ignorable id="11-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="12">
      <segment id="12-0">
        <source xml:space="preserve"> last:</source>
      </segment>
      <ignorable id="12-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="13">
      <segment id="13-0">
        <source xml:space="preserve"> Parametr vytvoří proměnnou typu Area, která odkazuje na poslední prvek daného seznamu oblastí.</source>
      </segment>
      <ignorable id="13-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="14">
      <segment id="14-0">
        <source xml:space="preserve"> [@SUPERAREAS last=my_area] </source>
      </segment>
    </unit>
    <unit id="15">
      <segment id="15-0">
        <source xml:space="preserve"> vytvoří proměnnou my_area jejíž hodnotou je poslední oblast z daného seznamu oblastí.</source>
      </segment>
      <ignorable id="15-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="16">
      <segment id="16-0">
        <source xml:space="preserve"> cond:</source>
      </segment>
      <ignorable id="16-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="17">
      <segment id="17-0">
        <source xml:space="preserve"> Parametr cond slouží k filtraci výběru oblastí.</source>
      </segment>
      <segment id="17-1">
        <source xml:space="preserve"> Jeho hodnotou je výraz, který musí povinně vracet booleovskou hodnotu (true nebo false).</source>
      </segment>
      <segment id="17-2">
        <source xml:space="preserve"> Oblast je zařazena do výsledného seznamu značky [@SUPERAREAS], je-li hodnota výrazu cond rovna hodnotě true.</source>
      </segment>
      <segment id="17-3">
        <source xml:space="preserve"> V opačném případě je oblast vynechána.</source>
      </segment>
      <segment id="17-4">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="17-5">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="18">
      <segment id="18-0">
        <source xml:space="preserve"> [@SUPERAREAS cond = "this.name.startsWith('my_')", ...</source>
      </segment>
      <segment id="18-1">
        <source xml:space="preserve"> ] </source>
      </segment>
    </unit>
    <unit id="19">
      <segment id="19-0">
        <source xml:space="preserve"> vybere pouze oblasti jejichž jméno začíná písmeny my_.</source>
      </segment>
      <ignorable id="19-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="20">
      <segment id="20-0">
        <source xml:space="preserve"> V parametru cond je vhodné použít objekt this, který odkazuje na aktuální oblast zpracovávanou značkou [@SUPERAREAS].</source>
      </segment>
      <segment id="20-1">
        <source xml:space="preserve"> Pomocí this lze filtrovat oblasti definované pomocí [@SUPERAREAS].</source>
      </segment>
      <segment id="20-2">
        <source xml:space="preserve"> Dalšími objekty, které lze použít pro filtrování oblastí jsou thisRel, thisIndex a thisSize.</source>
      </segment>
      <segment id="20-3">
        <source xml:space="preserve"> Hodnotou thisRel je objekt relace mezi oblastmi, který je typu AreaRelation.</source>
      </segment>
      <segment id="20-4">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="20-5">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="21">
      <segment id="21-0">
        <source xml:space="preserve"> [@SUPERAREAS cond = "thisRel.getSubName() = 'property'", ...</source>
      </segment>
      <segment id="21-1">
        <source xml:space="preserve"> ] </source>
      </segment>
    </unit>
    <unit id="22">
      <segment id="22-0">
        <source xml:space="preserve"> vybere pouze oblasti, k nímž vede relace pojmenovaná názvem property.</source>
      </segment>
      <ignorable id="22-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="23">
      <segment id="23-0">
        <source xml:space="preserve"> Hodnotou thisIndex je pořadí oblasti od 0 do N-1, kde N je počet oblastí a hodnotou thisSize je počet oblastí (N).</source>
      </segment>
      <segment id="23-1">
        <source xml:space="preserve"> Hodnota thisSize je konstantní.</source>
      </segment>
      <ignorable id="23-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="24">
      <segment id="24-0">
        <source xml:space="preserve"> reversed:</source>
      </segment>
      <ignorable id="24-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="25">
      <segment id="25-0">
        <source xml:space="preserve"> Parametr reversed zajistí na konci zpracování otočení pořadí oblastí v seznamu.</source>
      </segment>
      <ignorable id="25-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="26">
      <segment id="26-0">
        <source xml:space="preserve"> areaId:</source>
      </segment>
      <ignorable id="26-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="27">
      <segment id="27-0">
        <source xml:space="preserve"> Hodnota parametru areaId je číslo identifkující oblast.</source>
      </segment>
      <segment id="27-1">
        <source xml:space="preserve"> Tento identifikátor je oblasti přiřazen při jejím vzniku a jednoznačně ji určuje až do doby, kdy je smazána.</source>
      </segment>
      <segment id="27-2">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="27-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="28">
      <segment id="28-0">
        <source xml:space="preserve"> [@SUPERAREAS areaId = 1, ...] </source>
      </segment>
    </unit>
    <unit id="29">
      <segment id="29-0">
        <source xml:space="preserve"> - odkazuje na oblast s identifikátorem 1.</source>
      </segment>
      <ignorable id="29-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="30">
      <segment id="30-0">
        <source xml:space="preserve"> areaAlias:</source>
      </segment>
      <ignorable id="30-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="31">
      <segment id="31-0">
        <source xml:space="preserve"> Textová hodnota parametru areaAlias určuje zkratku identifikující oblast.</source>
      </segment>
      <segment id="31-1">
        <source xml:space="preserve"> Zkratka je pro oblast v daném projektu jednoznačná podobně jako identifikátor (ten je však jednoznačný pro všechny projekty).</source>
      </segment>
      <segment id="31-2">
        <source xml:space="preserve"> Nemusí však být u oblasti uvedena.</source>
      </segment>
      <segment id="31-3">
        <source xml:space="preserve"> Výhodou oproti identifikátoru oblasti je, že není zvolena automaticky programem, ale volí ji programátor.</source>
      </segment>
      <segment id="31-4">
        <source xml:space="preserve"> Zkratka se navíc může měnit v průběhu existence oblasti.</source>
      </segment>
      <segment id="31-5">
        <source xml:space="preserve"> Například značka:</source>
      </segment>
      <ignorable id="31-6">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="32">
      <segment id="32-0">
        <source xml:space="preserve"> [@SUPERAREAS areaAlias = #my_area, ...] </source>
      </segment>
    </unit>
    <unit id="33">
      <segment id="33-0">
        <source xml:space="preserve"> - odkazuje na oblast se zkratkou my_area.</source>
      </segment>
      <ignorable id="33-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="34">
      <segment id="34-0">
        <source xml:space="preserve"> Všimněte si znaku # před názvem zkratky.</source>
      </segment>
      <segment id="34-1">
        <source xml:space="preserve"> Je zde uveden proto, že vše co je za ním je interpretováno jako textový řetězec.</source>
      </segment>
      <segment id="34-2">
        <source xml:space="preserve"> Kdyby znak # nebyl před zkratkou uveden, bral by se text my_area jako pojmenování proměnné my_area, jejíž hodnota by určovala zkratku oblasti.</source>
      </segment>
      <ignorable id="34-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
