    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Tato jednoduchá značka ukončuje smyčku, kterou provádí značka [@LIST].</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Jakmile server narazí uvnitř značky [@LIST] na značku [@LAST], provede se aktuální iterace a příkaz [@LIST] se ukončí.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Následující program provede prvních deset iterací, pokud proměnná myAreas obsahuje deset a více prvků v seznamu:</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> [@LIST list=myAreas, iterator=myIter] [@IF cond=myIter.index=10] [@LAST] [/@IF] ...</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [/@LIST] Značku [@LAST] lze použít také ve žnačce [@LOOP].</source>
      </segment>
      <ignorable id="2-5">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@LAST] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> discard:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Parametr discard se zapisuje bez hodnoty a určuje, že poslední iterace příkazu [@LIST] bude zahozena.</source>
      </segment>
      <segment id="5-1">
        <source xml:space="preserve"> Následují program se provede pro prvních devět prvků seznamu v proměnné myAreas obsahuje-li 10 a více prvků:</source>
      </segment>
      <ignorable id="5-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> [@LIST list=myAreas, iterator=myIter] [@IF cond=myIter.index=10] [@LAST discard] [/@IF] ...</source>
      </segment>
      <segment id="6-1">
        <source xml:space="preserve"> [/@LIST] </source>
      </segment>
