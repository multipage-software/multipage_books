    <unit id="1">
      <segment id="1-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE] </source>
      </segment>
    </unit>
    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Vloží namísto značky textový obsah internetového souboru přiřazeného dané oblasti pomocí lokálního názvu.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Není-li oblast uvedena, bere se do úvahy aktuální oblast serveru.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Kódování textu se uvádí v parametru coding.</source>
      </segment>
      <ignorable id="2-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> res:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Parametr udává lokální název internetového souboru.</source>
      </segment>
      <segment id="5-1">
        <source xml:space="preserve"> Lokální název jednoznačně identifikuje internetový soubor, ve vztahu k dané oblasti.</source>
      </segment>
      <ignorable id="5-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> Následující značka odkazuje na internetový sobor s lokálním názvem my_image obsažený v aktuální oblasti serveru.</source>
      </segment>
      <ignorable id="6-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="7">
      <segment id="7-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE res = #my_image] </source>
      </segment>
    </unit>
    <unit id="8">
      <segment id="8-0">
        <source xml:space="preserve"> render:</source>
      </segment>
      <ignorable id="8-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="9">
      <segment id="9-0">
        <source xml:space="preserve"> Tento parametr je bez hodnoty.</source>
      </segment>
      <segment id="9-1">
        <source xml:space="preserve"> Je-li uveden parametr render, vytvoří se při generování statických stránek soubor odpovídající danému internetovému souboru i v případě, že není odkazován na některém místě na stránkách pomocí URL, tzn. že není z internetových stránek běžně dostupný.</source>
      </segment>
      <ignorable id="9-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="10">
      <segment id="10-0">
        <source xml:space="preserve"> coding:</source>
      </segment>
      <ignorable id="10-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="11">
      <segment id="11-0">
        <source xml:space="preserve"> Tento parametr určuje typ kódování při převodu obsahu internetového souboru do textu.</source>
      </segment>
      <segment id="11-1">
        <source xml:space="preserve"> Je platný pouze v případě, že je soubor uložen v binární formě.</source>
      </segment>
      <segment id="11-2">
        <source xml:space="preserve"> Typ kódování se uvádí jako řetězec stejně jako v prostředí jazyka Java, např. UTF-8, UTF-16, atd. </source>
      </segment>
    </unit>
    <unit id="12">
      <segment id="12-0">
        <source xml:space="preserve"> areaId:</source>
      </segment>
      <ignorable id="12-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="13">
      <segment id="13-0">
        <source xml:space="preserve"> Hodnota parametru areaId je číslo identifkující oblast.</source>
      </segment>
      <segment id="13-1">
        <source xml:space="preserve"> Tento identifikátor je oblasti přiřazen při jejím vzniku a jednoznačně ji určuje až do doby, kdy je smazána.</source>
      </segment>
      <segment id="13-2">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="13-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="14">
      <segment id="14-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE areaId = 1, ...] </source>
      </segment>
    </unit>
    <unit id="15">
      <segment id="15-0">
        <source xml:space="preserve"> - odkazuje na oblast s identifikátorem 1.</source>
      </segment>
      <ignorable id="15-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="16">
      <segment id="16-0">
        <source xml:space="preserve"> areaAlias:</source>
      </segment>
      <ignorable id="16-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="17">
      <segment id="17-0">
        <source xml:space="preserve"> Textová hodnota parametru areaAlias určuje zkratku identifikující oblast.</source>
      </segment>
      <segment id="17-1">
        <source xml:space="preserve"> Zkratka je pro oblast v daném projektu jednoznačná podobně jako identifikátor (ten je však jednoznačný pro všechny projekty).</source>
      </segment>
      <segment id="17-2">
        <source xml:space="preserve"> Nemusí však být u oblasti uvedena.</source>
      </segment>
      <segment id="17-3">
        <source xml:space="preserve"> Výhodou oproti identifikátoru oblasti je, že není zvolena automaticky programem, ale volí ji programátor.</source>
      </segment>
      <segment id="17-4">
        <source xml:space="preserve"> Zkratka se navíc může měnit v průběhu existence oblasti.</source>
      </segment>
      <segment id="17-5">
        <source xml:space="preserve"> Například značka:</source>
      </segment>
      <ignorable id="17-6">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="18">
      <segment id="18-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE areaAlias = #my_area, ...] </source>
      </segment>
    </unit>
    <unit id="19">
      <segment id="19-0">
        <source xml:space="preserve"> - odkazuje na oblast se zkratkou my_area.</source>
      </segment>
      <ignorable id="19-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="20">
      <segment id="20-0">
        <source xml:space="preserve"> Všimněte si znaku # před názvem zkratky.</source>
      </segment>
      <segment id="20-1">
        <source xml:space="preserve"> Je zde uveden proto, že vše co je za ním je interpretováno jako textový řetězec.</source>
      </segment>
      <segment id="20-2">
        <source xml:space="preserve"> Kdyby znak # nebyl před zkratkou uveden, bral by se text my_area jako pojmenování proměnné my_area, jejíž hodnota by určovala zkratku oblasti.</source>
      </segment>
      <ignorable id="20-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="21">
      <segment id="21-0">
        <source xml:space="preserve"> area:</source>
      </segment>
      <ignorable id="21-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="22">
      <segment id="22-0">
        <source xml:space="preserve"> Určuje oblast pomocí výrazu s výslednou hodnotou typu Area.</source>
      </segment>
      <segment id="22-1">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <ignorable id="22-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="23">
      <segment id="23-0">
        <source xml:space="preserve"> [@RESOURCE_VALUE area=subareas().getFirst(), ...] </source>
      </segment>
    </unit>
    <unit id="24">
      <segment id="24-0">
        <source xml:space="preserve"> V předchozím příkladu se použije první podoblast aktuální oblasti.</source>
      </segment>
      <segment id="24-1">
        <source xml:space="preserve"> Uvedený výraz subareas().getFirst() vrací objekt typu Area.</source>
      </segment>
      <ignorable id="24-2">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
