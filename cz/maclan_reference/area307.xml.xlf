    <unit id="21">
      <segment id="21-0">
        <source xml:space="preserve"> Tento parametr je bez hodnoty.</source>
      </segment>
      <segment id="21-1">
        <source xml:space="preserve"> Parametr určuje, že je-li v editoru slotu zadána neprázdná speciální hodnota (special value), použije se k vložení do textu tato speciální hodnota místo běžné hodnoty slotu.</source>
      </segment>
      <segment id="21-2">
        <source xml:space="preserve"> Speciální hodnota, není-li prázdná, tak v tomto případě přepisuje běžnou hodnotu slotu.</source>
      </segment>
      <ignorable id="21-3">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="22">
