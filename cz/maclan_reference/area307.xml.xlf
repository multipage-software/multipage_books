    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Značka [@TAG ...</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> ] vkládá na stránku obsah slotu dostupného (i dědičně) z dané oblasti.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Není-li uvedena oblast, bere se v úvahu aktuální oblast.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Není-li uveden parametr slot, bere se název prvního parametru jako název slotu.</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> Má-li slot název slot, musí se zapsat takto [@TAG slot=#slot] nikoliv [@TAG slot].</source>
      </segment>
      <ignorable id="2-5">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="3">
      <segment id="3-0">
        <source xml:space="preserve"> [@TAG] Má následující parametry:</source>
      </segment>
      <ignorable id="3-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="4">
      <segment id="4-0">
        <source xml:space="preserve"> slot:</source>
      </segment>
      <ignorable id="4-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="5">
      <segment id="5-0">
        <source xml:space="preserve"> Parametr udává název slotu.</source>
      </segment>
      <ignorable id="5-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="6">
      <segment id="6-0">
        <source xml:space="preserve"> Následující značka vypisuje na stránku obsah slotu s názvem mySlot, který je dostupný z aktuální zpracovávané oblasti serveru.</source>
      </segment>
      <ignorable id="6-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="7">
      <segment id="7-0">
        <source xml:space="preserve"> [@TAG slot = #mySlot] </source>
      </segment>
    </unit>
    <unit id="8">
      <segment id="8-0">
        <source xml:space="preserve"> Předchozí značka může být zkráceně zapsaná jako:</source>
      </segment>
      <ignorable id="8-1">
        <source xml:space="preserve"> </source>
      </ignorable>
    </unit>
    <unit id="9">
      <segment id="9-0">
        <source xml:space="preserve"> [@TAG mySlot] </source>
      </segment>
    </unit>
    <unit id="10">
