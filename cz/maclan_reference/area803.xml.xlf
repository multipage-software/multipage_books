    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Vkládá text do záložky s daným jménem.</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> Parametr name nebo název prvního parametru učuje jméno záložky.</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> Jde o složenou značku, jejíž obsah se provede, zapamatuje a na konci procesu se vloží namísto záložky.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> Například:</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [@REPLACE_BOOKMARK muj_text] "[@AREA_ALIAS homeArea]" [/@REPLACE_BOOKMARK] --- Alias domovské oblasti je:</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> [@BOOKMARK muj_text].</source>
      </segment>
      <segment id="2-6">
        <source xml:space="preserve"> - vypíše na stránku text:</source>
      </segment>
      <segment id="2-7">
        <source xml:space="preserve"> --- Alias domovské oblasti je:</source>
      </segment>
      <segment id="2-8">
        <source xml:space="preserve"> "documentation" .</source>
      </segment>
      <segment id="2-9">
        <source xml:space="preserve"> Použije-li se parametr name, pak bude předchozí příklad vypadat takto:</source>
      </segment>
      <segment id="2-10">
        <source xml:space="preserve"> [@REPLACE_BOOKMARK name=#muj_text] "[@AREA_ALIAS homeArea]" [/@REPLACE_BOOKMARK] --- Alias domovské oblasti je:</source>
      </segment>
      <segment id="2-11">
        <source xml:space="preserve"> [@BOOKMARK name=#muj_text].</source>
      </segment>
      <segment id="2-12">
        <source xml:space="preserve"> - vypíše na stránku opět text:</source>
      </segment>
      <segment id="2-13">
        <source xml:space="preserve"> --- Alias domovské oblasti je:</source>
      </segment>
      <segment id="2-14">
        <source xml:space="preserve"> "documentation" .</source>
