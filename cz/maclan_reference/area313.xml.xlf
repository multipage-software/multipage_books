    <unit id="2">
      <segment id="2-0">
        <source xml:space="preserve"> Obsah:</source>
      </segment>
      <segment id="2-1">
        <source xml:space="preserve"> [@LIST] ...</source>
      </segment>
      <segment id="2-2">
        <source xml:space="preserve"> [/@LIST] - Vkládá obsah seznamu.</source>
      </segment>
      <segment id="2-3">
        <source xml:space="preserve"> [@LAST] - Ukončuje blok [@LIST]...[/@LIST].</source>
      </segment>
      <segment id="2-4">
        <source xml:space="preserve"> [@LOOP] ...</source>
      </segment>
      <segment id="2-5">
        <source xml:space="preserve"> [/@LOOP] - Vkládá daný počet textů.</source>
      </segment>
      <segment id="2-6">
        <source xml:space="preserve"> [@IMAGE] - Vloží obrázek.</source>
      </segment>
      <segment id="2-7">
        <source xml:space="preserve"> [@VAR] - Definuje proměnné.</source>
      </segment>
      <segment id="2-8">
        <source xml:space="preserve"> [@SET] - Nastavuje proměnné na dané hodnoty.</source>
      </segment>
      <segment id="2-9">
        <source xml:space="preserve"> [@GET] - Vypíše hodnotu proměnné nebo výrazu.</source>
      </segment>
      <segment id="2-10">
        <source xml:space="preserve"> [@A] nebo [@ANCHOR] - Vloží odkaz.</source>
      </segment>
      <segment id="2-11">
        <source xml:space="preserve"> [@REM] ...</source>
      </segment>
      <segment id="2-12">
        <source xml:space="preserve"> [/@REM] - Vloží poznámku.</source>
      </segment>
      <segment id="2-13">
        <source xml:space="preserve"> [@IF] ...</source>
      </segment>
      <segment id="2-14">
        <source xml:space="preserve"> [@ELSEIF] ...</source>
      </segment>
      <segment id="2-15">
        <source xml:space="preserve"> [@ELSE] ...</source>
      </segment>
      <segment id="2-16">
        <source xml:space="preserve"> [/@IF] - Podmíněný příkaz.</source>
      </segment>
      <segment id="2-17">
        <source xml:space="preserve"> [@PROCEDURE] ...</source>
      </segment>
      <segment id="2-18">
        <source xml:space="preserve"> [/@PROCEDURE] - Definuje proceduru.</source>
      </segment>
      <segment id="2-19">
        <source xml:space="preserve"> [@CALL] nebo [@MojeProcedura] - Volání procedury.</source>
      </segment>
      <segment id="2-20">
        <source xml:space="preserve"> [@PACK]...[/@PACK] - Odstraní prázdné znaky.</source>
      </segment>
      <segment id="2-21">
        <source xml:space="preserve"> [@BLOCK]...[/@BLOCK] - Vytváří blok pro lokální proměnné.</source>
      </segment>
      <segment id="2-22">
        <source xml:space="preserve"> [@OUTPUT] ...</source>
      </segment>
      <segment id="2-23">
        <source xml:space="preserve"> [/@OUTPUT] - Zapisuje výsledek do externího provideru.</source>
      </segment>
      <segment id="2-24">
        <source xml:space="preserve"> [@TRACE] - Trasuje informace o daném místě v programu.</source>
      </segment>
      <segment id="2-25">
        <source xml:space="preserve"> [@BREAK] - Přeruší provádění programu na daném místě.</source>
      </segment>
      <segment id="2-26">
        <source xml:space="preserve"> [@PPTEXT] - Vlastnosti značek uvnitř příkazu mouhou obsahovat také značky (nerekurzivně).</source>
      </segment>
      <segment id="2-27">
        <source xml:space="preserve"> [@URL] - Vloží URL adresu.</source>
      </segment>
      <segment id="2-28">
        <source xml:space="preserve"> [@USING] - Deklaruje použití internetového souboru při renderování HTML stránek.</source>
      </segment>
      <segment id="2-29">
        <source xml:space="preserve"> [@RENDER_CLASS] - Nastaví název a popis třídy aktuální generované stránky.</source>
      </segment>
      <segment id="2-30">
        <source xml:space="preserve"> [@TIMOEOUT] - Nastaví timeout pro odezvu serveru v milisekundách.</source>
      </segment>
      <segment id="2-31">
        <source xml:space="preserve"> [@BOOKMARK] - Určuje místo, kde bude vložen text definovaný v jiné části programu.</source>
      </segment>
      <segment id="2-32">
        <source xml:space="preserve"> [@REPLACE_BOOKMARK] - Slouží k vložení textu do záložky s daným jménem.</source>
      </segment>
      <segment id="2-33">
        <source xml:space="preserve"> [@INCLUDE_ONCE] - Vkládá obsah značky do textu pouze jednou.</source>
      </segment>
      <segment id="2-34">
        <source xml:space="preserve"> [@LITERAL] - Zamezí zpracování příkazů uvnitř této složené značky.</source>
      </segment>
      <segment id="2-35">
        <source xml:space="preserve"> [@PRAGMA] - Pomocná značka.</source>
      </segment>
      <segment id="2-36">
        <source xml:space="preserve"> [@JAVASCRIPT] - Zpracovává program v JavaScriptu a generuje výstupní text. [@INDENT] ...</source>
      </segment>
      <segment id="2-37">
        <source xml:space="preserve"> [/@INDENT] - Zpracuje obsah a přidá odsazení na každou řádku.</source>
      </segment>
      <segment id="2-38">
        <source xml:space="preserve"> [@NOINDENT] ...</source>
      </segment>
      <segment id="2-39">
        <source xml:space="preserve"> [/@NOINDENT] - Zpracuje obsah a odstraní odsazení.</source>
      </segment>
      <segment id="2-40">
        <source xml:space="preserve"> [@CSS_LOOKUP_TABLE] - Definuje tabulku pro CSS oblast se sloty.</source>
      </segment>
      <segment id="2-41">
        <source xml:space="preserve"> [@UNZIP] - Extrahuje internetový soubor do adresáře aplikace.</source>
      </segment>
      <segment id="2-42">
        <source xml:space="preserve"> [@RUN] - Tato značka spouští příkaz operačního systému.</source>
      </segment>
      <segment id="2-43">
        <source xml:space="preserve"> [@REDIRECT] - Tato značka po ukončení metaprogramu přesměruje server na jinou stránku.</source>
      </segment>
      <segment id="2-44">
        <source xml:space="preserve"> @lb; - Vloží levou hranatou závorku.</source>
      </segment>
      <segment id="2-45">
        <source xml:space="preserve"> @rb; - Vloží pravou hranatou závorku.</source>
      </segment>
      <segment id="2-46">
        <source xml:space="preserve"> @at; - Vloží znak @.</source>
      </segment>
      <segment id="2-47">
        <source xml:space="preserve"> @l; - Vloží levou HTML závorku &lt;.</source>
      </segment>
      <segment id="2-48">
        <source xml:space="preserve"> @r; - Vloží pravou HTML závorku &gt;.</source>
      </segment>
      <segment id="2-49">
        <source xml:space="preserve"> [@TRAY_MENU] - Tato značka nastavuje menu na liště Windows.</source>
